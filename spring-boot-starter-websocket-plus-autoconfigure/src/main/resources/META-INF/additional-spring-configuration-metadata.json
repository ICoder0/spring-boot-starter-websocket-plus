{
  "groups": [
    {
      "name": "websocket-plus",
      "type": "com.icoder0.websocket.spring.WebsocketPlusProperties",
      "description": "Description for websocket-plus."
    }
  ],
  "properties": [
    {
      "name": "websocket-plus.asyncSendTimeout",
      "type": "java.lang.Long",
      "defaultValue": 5000,
      "description": "async send timeout"
    },
    {
      "name": "websocket-plus.maxSessionIdleTimeout",
      "type": "java.lang.Long",
      "defaultValue": 60000,
      "description": "websocketSession最大闲置超时时间"
    },
    {
      "name": "websocket-plus.maxTextMessageBufferSize",
      "type": "java.lang.Integer",
      "defaultValue": 10240,
      "description": "最大文本内容缓冲区大小限制"
    },
    {
      "name": "websocket-plus.maxBinaryMessageBufferSize",
      "type": "java.lang.Integer",
      "defaultValue": 10240,
      "description": "最大二进制内容缓冲区大小限制"
    },
    {
      "name": "websocket-plus.withSockJS",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "是否需要开启sockjs"
    },
    {
      "name": "websocket-plus.origins",
      "defaultValue": ["*"],
      "description": "跨域origins集合"
    },
    {
      "name": "websocket-plus.inboundBeanClazz",
      "type": "java.lang.Class",
      "defaultValue": "com.icoder0.websocket.core.model.WsInboundBean",
      "description": "上行数据格式解析类"
    },
    {
      "name": "websocket-plus.outboundBeanClazz",
      "type": "java.lang.Class",
      "defaultValue": "com.icoder0.websocket.core.model.WsOutboundBean",
      "description": "下行数据格式解析类"
    },
    {
      "name": "websocket-plus.inboundSpecification",
      "type": "java.lang.String",
      "defaultValue": "{seq:0, code:1000, version:0, params:{}}",
      "description": "上行数据格式规范"
    },
    {
      "name": "websocket-plus.outboundSpecification",
      "type": "java.lang.String",
      "defaultValue": "{seq:0, code:200, message:{\"action success\"}, content:{}}",
      "description": "下行数据格式规范"
    },
    {
      "name": "websocket-plus.payloadParamsDecodeName",
      "type": "java.lang.String",
      "defaultValue": "sequence",
      "description": "上行数据格式#业务参数对应的变量名"
    },
    {
      "name": "websocket-plus.payloadSequenceDecodeName",
      "type": "java.lang.String",
      "defaultValue": "params",
      "description": "上行数据格式#消息序号对应的变量名"
    },
    {
      "name": "websocket-plus.payloadFunctionCodeDecodeName",
      "type": "java.lang.String",
      "defaultValue": "code",
      "description": "上行数据格式#函数枚举对应的变量名"
    },
    {
      "name": "websocket-plus.spelVariableName",
      "type": "java.lang.String",
      "defaultValue": "inbound",
      "description": "spel#expression绑定的变量名"
    }
  ]
}
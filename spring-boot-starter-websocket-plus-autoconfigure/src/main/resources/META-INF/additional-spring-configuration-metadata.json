{
  "groups": [
    {
      "name": "websocket-plus",
      "type": "com.icoder0.websocket.spring.WebsocketPlusProperties",
      "description": "Description for websocket-plus."
    }
  ],
  "properties": [
    {
      "name": "websocket-plus.asyncSendTimeout",
      "type": "java.lang.Long",
      "defaultValue": 5000,
      "description": "async send timeout"
    },
    {
      "name": "websocket-plus.maxSessionIdleTimeout",
      "type": "java.lang.Long",
      "defaultValue": 60000,
      "description": "websocketSession最大闲置超时时间"
    },
    {
      "name": "websocket-plus.maxTextMessageBufferSize",
      "type": "java.lang.Integer",
      "defaultValue": 10240,
      "description": "最大文本内容缓冲区大小限制"
    },
    {
      "name": "websocket-plus.maxBinaryMessageBufferSize",
      "type": "java.lang.Integer",
      "defaultValue": 10240,
      "description": "最大二进制内容缓冲区大小限制"
    },
    {
      "name": "websocket-plus.withSockJS",
      "type": "java.lang.Boolean",
      "defaultValue": false,
      "description": "是否需要开启sockjs"
    },
    {
      "name": "websocket-plus.origins",
      "defaultValue": ["*"],
      "description": "跨域origins集合"
    },
    {
      "name": "websocket-plus.outerDecodeClazz",
      "type": "java.lang.Class",
      "defaultValue": "com.icoder0.websocket.core.model.WsInboundBean",
      "description": "textMessage最外层类的类型"
    },
    {
      "name": "websocket-plus.innerDecodeParamKeyName",
      "type": "java.lang.String",
      "defaultValue": "params",
      "description": "最大二进制内容缓冲区大小限制"
    },
    {
      "name": "websocket-plus.spelRootName",
      "type": "java.lang.String",
      "defaultValue": "inbound",
      "description": "WebsocketMethodMapping属性value中的context变量名"
    },
    {
      "name": "websocket-plus.payloadSpecification",
      "type": "java.lang.String",
      "defaultValue": "{seq:0, code:1000, version:0, params:{}}",
      "description": "Websocket#TextMessage上行数据格式规范"
    },
    {
      "name": "websocket-plus.payloadParamSpecification",
      "type": "java.lang.String",
      "defaultValue": "{..., params:{}}",
      "description": "Websocket#TextMessage上行数据params字段格式规范"
    }
  ]
}